

     Named Company GM
     do: [ 
         !dr <- 90 to: 88 by: 1 yearEnds ;          #--  create date range
     
         " " print: 15 ;                            #-- indent
         dr evaluate: [ ^date year print: 10 ; ] ;  #-- display years
         newLine print ;  
     
         "Sales" print: 15 ;
         dr evaluate: [ sales print: 10 ] ;
         newLine print ;
     
         "EPS" print: 15 ;
         dr evaluate: [ earningsPerShare print: 10 ] ;
         newLine print ;
     
         "Price" print: 15 ;
         dr evaluate: [ price print: 10 ] ;
         newLine print ;
         ] ;
        


